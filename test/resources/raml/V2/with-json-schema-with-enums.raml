#%RAML 1.0
---
title: With json schama
version: 1.0

/my/endpoint:
    post:
      description: 
      displayName: 
      body:
        application/json:
          type: |
            { "$schema": "http://json-schema.org/schema",
              "type": "string",
              "description": "my enums field",
              "oneOf": [
                { "enum": ["enum-a", "enum-b"] }
              ],
              "required": [ "id", "name" ]
            }

